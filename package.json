{
  "name": "lifi-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "agent": "concurrently \"tsx src/index.ts\" \"make-agent dev\"",
    "api": "tsx src/index.ts",
    "build": "tsc",
    "deploy": "make-agent deploy -u https://lifi-agent.vercel.app/",
    "lint": "biome check && prettier --check '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "biome format --write . && prettier --write '{src,tests}/**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.4.1",
    "@lifi/sdk": "^3.13.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "viem": "^2.39.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.5",
    "biome": "^0.3.3",
    "concurrently": "^9.2.1",
    "make-agent": "^0.3.5",
    "prettier": "^3.6.2",
    "typescript": "5.9.3"
  }
}
