    "redoc": "^2.5.0",
    "vercel-url": "^0.2.8",
    "viem": "^2.31.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^24.0.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.3",
    "make-agent": "0.3.1",
    "prettier": "^3.5.3",
    "typescript": "5.8.3"
  }
